#include<stdio.h>
#include<stdlib.h>

typedef union Z Z;
union Z
{
  char ch;
  unsigned char uc;
  short sh;
  unsigned short us;
  int in;
  unsigned int ui;
  long lo;
  unsigned long ul;
  long long ll;
  unsigned long long ull;
  float fl;
  double db;
  long double ld;
  union Z *next;
  union Z (*zero)();
  union Z (*one)(union Z);
  union Z (*two)(union Z,union Z);
  union Z (*three)(union Z,union Z,union Z);
  union Z (*four)(union Z,union Z,union Z,union Z);
  union Z (*five)(union Z,union Z,union Z,union Z,union Z);
  union Z (*six)(union Z,union Z,union Z,union Z,union Z,union Z);
  union Z (*seven)(union Z,union Z,union Z,union Z,union Z,union Z,union Z);
  union Z (*eight)(union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z);
  union Z (*nine)(union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z);
  union Z (*ten)(union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z);
};
typedef union Z z;

z ident(z one)
{
  return one;
}

int main()
{
  z value;
  z function;
  value.uc='Z';
  function.one=&ident;

  value=function.one(value);
  printf("%c\n",value.uc);
  printf("%d\n",sizeof(z));
  printf("%d\n",sizeof(char));
  return 0;
}
