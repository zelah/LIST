#include<stdio.h>
#include<stdlib.h>

typedef union Z Z;
union Z
{
  char ch;
  unsigned char uc;
  short sh;
  unsigned short us;
  int in;
  unsigned int ui;
  long lo;
  unsigned long ul;
  long long ll;
  unsigned long long ull;
  float fl;
  double db;
  long double ld;
  union Z *next;
  union Z (*f0)();
  union Z (*f1)(union Z);
  union Z (*f2)(union Z,union Z);
  union Z (*f3)(union Z,union Z,union Z);
  union Z (*f4)(union Z,union Z,union Z,union Z);
  union Z (*f5)(union Z,union Z,union Z,union Z,union Z);
  union Z (*f6)(union Z,union Z,union Z,union Z,union Z,union Z);
  union Z (*f7)(union Z,union Z,union Z,union Z,union Z,union Z,union Z);
  union Z (*f8)(union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z);
  union Z (*f9)(union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z);
  union Z (*f10)(union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z);
  union Z (*f11)(union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z);
  union Z (*f12)(union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z);
  union Z (*f13)(union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z);
  union Z (*f14)(union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z);
  union Z (*f15)(union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z);
  union Z (*f16)(union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z);
  union Z (*f17)(union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z);
  union Z (*f18)(union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z);
  union Z (*f19)(union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z);
  union Z (*f20)(union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z,union Z);
};
typedef union Z z;

z ident(z one)
{
  return one;
}

int main()
{
  z value;
  z f;
  value.uc='Z';
  f.f1=&ident;

  value=f.f1(value);
  printf("%c\n",value.uc);
  printf("%d\n",sizeof(z));
  printf("%d\n",sizeof(char));
  return 0;
}
